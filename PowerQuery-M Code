//Updated 3/24/2021 by Matt Miller
let
    Source = Table.FromRows(Json.Document(Binary.Decompress(Binary.FromText("", BinaryEncoding.Base64), Compression.Deflate)), let _t = ((type nullable text) meta [Serialized.Text = true]) in type table [RCCC = _t, Unit = _t, Group = _t]),
    #"Changed Type" = Table.TransformColumnTypes(Source,{{"RCCC", type text}, {"Unit", type text}, {"Group", type text}}),
    #"Renamed Columns" = Table.RenameColumns(#"Changed Type",{{"Unit", "Unit-RCCC"}, {"Group", "Group-RCCC"}}),
    #"Filtered Rows" = Table.SelectRows(#"Renamed Columns", each ([#"Group-RCCC"] <> "BMT" and [#"Group-RCCC"] <> "CMS" and [#"Group-RCCC"] <> "IDT" and [#"Group-RCCC"] <> "IRT" and [#"Group-RCCC"] <> "PTP" and [#"Group-RCCC"] <> "STP1" and [#"Group-RCCC"] <> "STP2" and [#"Group-RCCC"] <> "TAP"))
in
    #"Filtered Rows"
